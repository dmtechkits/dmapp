{
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  },
  "rules": {
    "import-name": false,
    "no-console": false,
    "no-submodule-imports": false,
    "variable-name": false,
    "object-literal-sort-keys": false,
    "member-access": false
  }
}
