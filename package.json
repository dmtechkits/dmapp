{
  "name": "dmapp",
  "description": "next.js ts starter",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node .next/server",
    "lint": "tslint --project ./tsconfig.json -c tslint.json --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@rebass/grid": "^6.0.0",
    "express": "^4.17.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sitemap": "^2.2.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/express": "^4.16.1",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.8",
    "@types/rebass__grid": "^6.0.5",
    "@types/styled-components": "^4.1.15",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "license": "MIT"
}
